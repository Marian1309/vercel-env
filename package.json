{
  "name": "@pidchashyi/vercel-env",
  "version": "1.1.0",
  "description": "A comprehensive library and CLI tool for syncing environment variables between local files and Vercel",
  "keywords": [
    "vercel",
    "environment",
    "env",
    "sync",
    "cli",
    "deployment",
    "variables"
  ],
  "homepage": "https://github.com/Marian1309/vercel-env#readme",
  "bugs": {
    "url": "https://github.com/Marian1309/vercel-env/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Marian1309/vercel-env.git"
  },
  "license": "MIT",
  "author": "Marian Pidchashyi",
  "type": "module",
  "bin": {
    "vercel-env": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./sync": {
      "import": "./dist/sync.js",
      "require": "./dist/sync.js",
      "types": "./dist/sync.d.ts"
    },
    "./delete": {
      "import": "./dist/delete.js",
      "require": "./dist/delete.js",
      "types": "./dist/delete.d.ts"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepare": "bun run build",
    "postpublish": "rimraf -rf dist",
    "sync": "bun src/actions/sync.ts",
    "delete": "bun src/actions/delete.ts",
    "sync:dev": "bun src/sync.ts --dev --auto",
    "sync:prod": "bun src/actions/sync.ts --prod --interactive",
    "delete:dev": "bun src/actions/delete.ts --dev",
    "delete:prod": "bun src/actions/delete.ts --prod"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "vercel": "^48.1.6"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
